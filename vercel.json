{
  "buildCommand": "echo 'Frontend deployment'",
  "outputDirectory": "public",
  "routes": [
    {
      "source": "/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard.html",
      "destination": "/dashboard.html"
    },
    {
      "source": "/people.html",
      "destination": "/people.html"
    },
    {
      "source": "/history.html",
      "destination": "/history.html"
    },
    {
      "source": "/css/(.*)",
      "destination": "/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/js/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
